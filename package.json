{
  "name": "nolo-ssr",
  "version": "0.0.1",
  "description": "nolo create it for ssr",
  "main": "server/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ]
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "emotion": "^10.0.27",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-find": "^7.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "webpack": "^5.7.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-hyperscript": "^0.59.0",
    "slate-react": "^0.59.0"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*",
      "pages/*",
      "components/*",
      "template/*"

    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "start": "npm run prod",
    "server": "node ./build/server.js",
    "dev": "cross-env NODE_ENV=development  npm-run-all  build:client build:server server ",
    "prod": "cross-env NODE_ENV=production npm-run-all build:client build:server server",
    "build:server": "webpack  --config config/webpack.server.js ",
    "build:client": "webpack  --config config/webpack.client.js ",
    "clean": "rimraf build",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolotus/nolo-ssr.git"
  },
  "keywords": [
    "ssr"
  ],
  "author": "nolotus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nolotus/nolo-ssr/issues"
  },
  "homepage": "https://github.com/nolotus/nolo-ssr#readme"
}
